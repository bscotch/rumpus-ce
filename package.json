{
  "name": "@bscotch/rumpus-ce",
  "version": "1.2.4",
  "description": "Rumpus Community Edition SDK (Rumpus CE SDK), for creating browser and Node.js applications that use Levelhead data.",
  "main": "build/index.js",
  "browser": "browser/index.js",
  "files": [
    "build/lib/**/*",
    "build/index.*",
    "build/types/**/*"
  ],
  "scripts": {
    "test": "npx mocha --inspect --require source-map-support/register --bail  ./build/test/index.js",
    "build": "rm -rf build && npx tsc && npx webpack",
    "preversion": "git checkout develop && git fetch origin --prune && npm run build && npm test",
    "version": "git add -A",
    "postversion": "git push origin develop && git checkout master && git merge develop && git push origin master && npm publish && git checkout develop"
  },
  "keywords": [
    "rumpus",
    "api",
    "bscotch",
    "butterscotch shenanigans",
    "levelhead",
    "levels"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bscotch/rumpus-ce.git"
  },
  "author": "Adam Coster",
  "license": "MIT",
  "homepage": "https://github.com/bscotch/rumpus-ce#readme",
  "dependencies": {
    "axios": "^0.22.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.17.20",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "esbuild": "^0.13.3",
    "eslint": "^7.32.0",
    "mocha": "^9.1.2",
    "prettier": "^2.4.1",
    "source-map-support": "^0.5.20",
    "typescript": "^4.4.3"
  }
}
